{
  "name": "katsu-curry",
  "version": "0.7.6",
  "description": "curry functions with placeholders or in object form",
  "license": "ISC",
  "repository": "brekk/katsu-curry",
  "author": "brekk",
  "main": "katsu-curry.js",
  "scripts": {
    "docs": "nps docs",
    "dependencies": "nps dependencies",
    "readme": "nps readme",
    "lint": "nps lint",
    "test": "nps test",
    "build": "nps build",
    "care": "nps care",
    "precommit": "nps precommit"
  },
  "files": [
    "katsu-curry.*",
    "debug.js",
    "fp",
    "object",
    "params",
    "placeholder",
    "utils",
    "debug"
  ],
  "keywords": [
    "curry",
    "object-style curry",
    "partial application"
  ],
  "dependencies": {
    "fast.js": "^0.1.1"
  },
  "devDependencies": {
    "@ibrokethat/curry": "^1.0.3",
    "@riim/curry": "^1.0.0",
    "auto-curry": "^0.2.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-fbjs": "^2.1.4",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "benchmark": "^2.1.4",
    "bloody-curry": "^1.0.1",
    "bluebird": "^3.5.0",
    "cast-curry": "^1.0.0",
    "clinton": "^0.13.0",
    "coveralls": "^3.0.0",
    "curri": "^1.0.0",
    "curry": "^1.2.0",
    "curry-d": "^0.6.0",
    "dead-simple-curry": "^1.1.2",
    "del-cli": "^1.1.0",
    "depcheck": "^0.6.7",
    "documentation": "^4.0.0-rc.1",
    "docusaurus": "^1.0.7",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-better": "^0.1.5",
    "eslint-plugin-eslint-comments": "^1.0.2",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^2.0.1",
    "execa": "^0.8.0",
    "fj-curry": "^1.0.0",
    "fjl-curry": "^1.0.2",
    "fpo": "^6.1.6",
    "germs": "^0.1.9",
    "husky": "^0.14.2",
    "instant-curry": "^1.2.0",
    "jayin": "^0.0.3",
    "jest": "^21.2.1",
    "jest-alias-preprocessor": "^1.1.1",
    "jest-cli": "^21.2.1",
    "jest-junit-reporter": "^1.1.0",
    "jest-t-assert": "^0.2.0",
    "just-curry-it": "^3.0.0",
    "light-curry": "^0.0.2",
    "lodash": "^4.17.4",
    "lodash.once": "^4.1.1",
    "lodash.padend": "^4.6.1",
    "madge": "^2.0.0",
    "microtime": "^2.1.6",
    "minimist": "^1.2.0",
    "moize": "^4.0.4",
    "npm-merge-driver": "^2.3.5",
    "nps": "^5.5.0",
    "nps-utils": "^1.2.0",
    "nyc": "^11.0.3",
    "ramda": "^0.24.1",
    "remark-cli": "^5.0.0",
    "remark-validate-links": "^7.0.0",
    "replacestream": "^4.0.3",
    "rollup": "^0.49.2",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-babili": "^3.1.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-prepack": "^1.0.0",
    "rollup-plugin-progress": "^0.4.0",
    "testperf": "^2.0.3",
    "wallaby-webpack": "^0.0.39",
    "webpack": "2.2.x"
  },
  "module": "katsu-curry.mjs",
  "reveal": true,
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coveragePathIgnorePatterns": [],
    "mapCoverage": true,
    "moduleNameMapper": {
      "@combinators/(.*)\\.(.*)": "<rootDir>/src/combinators/$1.$2",
      "@params/(.*)\\.(.*)": "<rootDir>/src/params/$1.$2",
      "@fp/(.*)\\.(.*)": "<rootDir>/src/fp/$1.$2",
      "@object/(.*)\\.(.*)": "<rootDir>/src/object/$1.$2",
      "@placeholder/(.*)\\.(.*)": "<rootDir>/src/placeholder/$1.$2",
      "@utils/(.*)\\.(.*)": "<rootDir>/src/utils/$1.$2"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testMatch": [
      "**/*.spec.(ts|tsx|js)"
    ]
  },
  "clinton": {
    "inherit": false,
    "rules": {
      "cli": "error",
      "editorconfig": "off",
      "filename-case": [
        "warn",
        {
          "case": "kebabCase"
        }
      ],
      "gulp": [
        "error",
        "optional"
      ],
      "keywords": "error",
      "license": "ISC",
      "max-depth": "warn",
      "no-callback": "error",
      "no-dup-keywords": "error",
      "no-empty-keywords": "error",
      "no-git-merge-conflict": "error",
      "pkg-dependency-order": "error",
      "pkg-description": "off",
      "pkg-engine": "off",
      "pkg-main": "error",
      "pkg-name": "error",
      "pkg-normalize": "warn",
      "pkg-property-order": "error",
      "pkg-schema": "error",
      "pkg-shorthand-repository": "error",
      "pkg-user-order": "error",
      "readme": "error",
      "test-script": "error",
      "use-travis": "off",
      "valid-version": "error",
      "ava": "off",
      "gitignore": "error",
      "xo": "off"
    }
  },
  "nyc": {
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "reporter": [
      "lcov",
      "json",
      "html",
      "text",
      "text-summary"
    ],
    "exclude": [
      "node_modules",
      "config",
      "coverage",
      "src/*.spec.js",
      "src/*.fixture.js",
      "src/**/index.js",
      "src/index.js",
      "src/combinators.js",
      "katsu-curry.js",
      "katsu-curry.es.js"
    ],
    "include": [
      "src/*.js",
      "src/**/*.js",
      "!src/*.spec.js"
    ],
    "all": true,
    "check-coverage": true,
    "require": [
      "babel-register"
    ]
  },
  "tonicExampleFilename": "runkit-example.js"
}
