{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "lib": [
      "dom",
      "es2015.collection",
      "es2015.iterable",
      "es2015.promise",
      "es2015",
      "es2016",
      "es2017",
      "es5"
    ],
    "module": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "target": "es6",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [

    ],
    "paths": {
      "@katsu/*": [ "src/*" ],
      "@combinators/*": [ "src/combinators/*" ],
      "@curry/*": [ "src/curry/*" ],
      "@fp/*": [ "src/fp/*" ],
      "@object/*": [ "src/object/*" ],
      "@placeholder/*": [ "src/placeholder/*" ],
      "@utils/*": [ "src/utils/*" ]
    }
  },
  "files": [
    "src/typings.d.ts",
    "custom.d.ts",
    "src/combinators/*",
    "src/curry/*",
    "src/fp/*",
    "src/object/*",
    "src/placeholder/*",
    "src/utils/*",
    "src/*.js"
  ],
  "exclude": [
    "*.spec.*",
    "node_modules"
  ],
  "awesomeTypescriptLoaderOptions": {
    "useBabel": true,
    "useWebpackText": true
  }
}
