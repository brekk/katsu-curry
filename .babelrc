{
  "sourceMaps": true,
  "presets": ["es2015", "node6"],
  "plugins": [
    "transform-object-rest-spread",
    "transform-es2015-destructuring",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@katsu": "./src",
        "@combinators": "./src/combinators",
        "@params": "./src/params",
        "@fp": "./src/fp",
        "@object": "./src/object",
        "@placeholder": "./src/placeholder",
        "@utils": "./src/utils"
      }
    }]
  ],
  "env": {
    "production": {
      "modules": "commonjs"
    }
  }
}
